#include "skeletyl.dtsi"

&kscan0 {
    row-gpios
        = <&pro_micro 4 (GPIO_ACTIVE_HIGH)> // row0
        , <&pro_micro 5 (GPIO_ACTIVE_HIGH)> // row1
        , <&pro_micro 6 (GPIO_ACTIVE_HIGH)> // row2
        , <&pro_micro 7 (GPIO_ACTIVE_HIGH)> // row3
        ; 
    col-gpios
        = <&pro_micro 19 (GPIO_ACTIVE_LOW | GPIO_PULL_UP)> // col0 - D19
        , <&pro_micro 18 (GPIO_ACTIVE_LOW | GPIO_PULL_UP)> // col1 - D18
        , <&pro_micro 15 (GPIO_ACTIVE_LOW | GPIO_PULL_UP)> // col2 - D15
        , <&pro_micro 14 (GPIO_ACTIVE_LOW | GPIO_PULL_UP)> // col3 - D14
        , <&pro_micro 16 (GPIO_ACTIVE_LOW | GPIO_PULL_UP)> // col4 - D16
        ;
};
